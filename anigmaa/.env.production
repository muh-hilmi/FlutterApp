# Production Environment Configuration
APP_ENV=production

# API Configuration
API_BASE_URL=https://api.anigmaa.com
API_VERSION=v1

# Feature Flags
ENABLE_DARK_MODE=true
ENABLE_PUSHLNOTIFICATIONS=true
ENABLE_LOCATION_SERVICES=true
ENABLE_ANALYTICS=true

# Security (Production values - REPLACE WITH ACTUAL SECRETS!)
# Generate strong secrets with: openssl rand -base64 32
ENCRYPTION_KEY=PASTE_YOUR_32_CHAR_ENCRYPTION_KEY_HERE
JWT_SECRET=PASTE_YOUR_32_CHAR_JWT_SECRET_HERE

# External Services (Production)
SENTRY_DSN=https://YOUR_SENTRY_DSN_HERE
FIREBASE_PROJECT_ID=anigmaa-production

# Rate Limiting
RATE_LIMIT_ENABLED=true
RATE_LIMIT_REQUESTS_PER_MINUTE=60

# Cache Configuration
CACHE_TTL_SECONDS=300
CACHE_ENABLED=true
